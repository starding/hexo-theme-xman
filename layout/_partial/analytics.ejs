<% if (theme.google_analytics_4) { %>
 <!-- Google tag (gtag.js) -->
    <script async
        src="https://www.googletagmanager.com/gtag/js?id=<%= theme.google_analytics_4.TAG_ID %>">
    </script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() {
            dataLayer.push(arguments);
        }
        gtag('js', new Date());
        gtag('config',
        '<%= theme.google_analytics_4.TAG_ID %>');
    </script>
<% } %>
